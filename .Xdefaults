XTerm*faceName: Consolas
XTerm*faceSize: 14
Xterm*font: Consolas:pixelsize=14
XTerm*saveLines: 16384
XTerm*geometry: 100x25-500+400
XTerm*VT100*translations: #override\nShift Ctrl <KeyPress> v:insert-selection(CLIPBOARD)\nShift Ctrl <KeyPress> c:copy-selection(CLIPBOARD)
!XTerm*VT100*translations: #override Shift <Btn1Up>: exec-formatted("chromium '%t'", PRIMARY)
XTerm*boldMode: false
XTerm*allowBoldFonts: false
XTerm*background: #181818
XTerm*foreground: #ffffff
xterm*locale: true
xterm*utf8: 1
xterm*termName: xterm-256color


