! xft.antialias:  true
! xft.hinting:    false
! xft.hintstyle:  hintslight
! xft.rgba:       none
! xft:            unifont:medium


!                                   __   ___
!                                   \ \ / / |
!                                    \ V /| |_ ___ _ __ _ __ ___
!                                    /   \| __/ _ \ '__| '_ ` _  \
!                                   / /^\ \ ||  __/ |  | | | | | |
!                                   \/   \/\__\___|_|  |_| |_| |_|

xterm*font:                 -*-tamzenforpowerline-medium-*-*-*-20-*-*-*-*-*-*-*
xterm*blodFont:             -*-tamzenforpowerline-bold-*-*-*-20-*-*-*-*-*-*-*
xterm*saveLines:            0
xterm*geometry:             80x24-500+400
xterm*allowBoldFonts:       true
xterm*utf8:                 true
xterm*termName:             xterm-256color
xterm*VT100*translations:   #override\nShift Ctrl <KeyPress> v:insert-selection(CLIPBOARD)\nShift Ctrl <KeyPress> c:copy-selection(CLIPBOARD)

!                                     _   _                  _
!                                    | | | |                | |
!                                    | | | |_ ____  ____   _| |_
!                                    | | | |  __\ \/ /\ \ / / __|
!                                    | |_| | |   >  <  \ V /| |_
!                                     \___/|_|  /_/\_\  \_/  \__|

urxvt.buffered:             false
! use tmux scroll buffer instead
urxvt*saveLines:            999
urxvt.letterSpace:          -1
urxvt.cursorUnderline:      true
urxvt.cursorBlink:          false
urxvt.urgentOnBell:         true
urxvt.inheritPixmap:        false
urxvt.geometry:             80x24-500+400
urxvt.scrollBar:            false
urxvt.depth:                32
urxvt.imLocale:             en_US.UTF-8
urxvt.borderLess:           false
urxvt.pointerBlank:         true
urxvt.pointerBlankDelay:    4
urxvt*iconFile:             /usr/share/icons/FlatWoken/scalable/apps/xterm.svg
! no ISO 14755 mode (fix for C-M-v)
urxvt.iso14755:             false
urxvt.iso14755_52:          false
! do not scroll with output
URxvt*scrollTtyOutput:      false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer:     true
! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress:    true
! font settings
! urxvt.font:                 -*-tamzenforpowerline-medium-*-*-*-20-*-*-*-*-*-*-*,xft:Hasklig Nerd Font:size=20

! urxvt.font: -gohu-gohufont-medium-r-normal--14-100-100-100-c-0-iso8859-1,xft:SourceCodePro+Powerline+Awesome\ Regular:size=12:antialias=true:hinting=true

urxvt.font:                 -*-tamzenforpowerline-medium-*-*-*-20-*-*-*-*-*-*-*,xft:SourceCodePro+Powerline+Awesome\ Regular:size=12:antialias=true:hinting=true

! urxvt.font: -*-tamzenforpowerline-medium-*-*-*-20-*-*-*-*-*-*-*,xft:FantasqueSansMono Nerd Font:Regular:pixelsize=38:antialias=true:hinting=true
! urxvt.boldFont:             xft:Monaco\ for\ Powerline:style=dz:style=Bold:pixelsize=17:autohint=true
! urxvt.italicFont:           xft:Monaco\ for\ Powerline:style=Italic:pixelsize=17:autohint=true
! urxvt.boldItalicFont:       xft:Monaco\ for\ Powerline:style=Bold:style=Italic:pixelsize=17:autohint=true
! URxvt*font: -gohu-gohufont-medium-r-normal--14-100-100-100-c-80-iso10646-1
! URxvt*font: xft:Inconsolata -g:style=g:pixelsize=18
! Urxvt extensions requires urxvt-perl package
urxvt.perl-ext-common:      default -searchable-scrollback,matcher,keyboard-select,clipboard
urxvt.keysym.C-x:           perl:keyboard-select:activate
urxvt.keysym.M-u:           perl:url-select:select_next
urxvt.keysym.C-b:           perl:clipboard:paste_escaped
urxvt.matcher.button:       1
urxvt.matcher.rend.0:       -Uline fg16
urxvt.urlLauncher:          /usr/bin/chromium
URxvt.url-select.underline: true
URxvt.url-select.launcher:  /usr/bin/xdg-open
urxvt.clipboard.autocopy:   true
urxvt.clipboard.copycmd:    xsel --input  --clipboard
urxvt.clipboard.pastecmd:   xsel --output --clipboard
urxvt.keysym.C-M-c:         perl:clipboard:copy
urxvt.keysym.C-V:           perl:clipboard:paste
urxvt.keysym.M-r:           perl:keyboard-select:search
urxvt.font-size.step:       4
urxvt.intensityStyles:      false
URxvt.keysym.Home:          \033[1~
URxvt.keysym.End:           \033[4~
URxvt.keysym.KP_Home:       \033[1~
URxvt.keysym.KP_End:        \033[4~


