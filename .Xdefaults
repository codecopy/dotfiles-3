xterm*faceName:             Inconsolata for Powerline
xterm*scaleHeight:          1.01 
xterm*faceSize:             16
xterm*saveLines:            16384
xterm*geometry:             80x24-500+400
xterm*VT100*translations:   #override\nShift Ctrl <KeyPress> v:insert-selection(CLIPBOARD)\nShift Ctrl <KeyPress> c:copy-selection(CLIPBOARD)
xterm*boldMode:             false
xterm*allowBoldFonts:       false
xterm*utf8:                 true
xterm*termName:             xterm-256color
!xterm.VT100*utf8:           1

!                                     _   _                  _   
!                                    | | | |                | |  
!                                    | | | |_ ____  ____   _| |_ 
!                                    | | | |  __\ \/ /\ \ / / __|
!                                    | |_| | |   >  <  \ V /| |_ 
!                                     \___/|_|  /_/\_\  \_/  \__|

! urxvt.letterSpace:          -1.5
urxvt.buffered:             false
! urxvt.cursorUnderline:      false
urxvt.cursorBlink:          false
urxvt.urgentOnBell:         true
urxvt.inheritPixmap:        false
urxvt.geometry:             80x24-500+400
urxvt.scrollBar:            false
urxvt.saveLines:            12000
urxvt.depth:                32
urxvt.imLocale:             en_US.UTF-8
urxvt.borderLess:           false
urxvt.pointerBlank:         true
urxvt.pointerBlankDelay:    4
urxvt*iconFile:             /usr/share/icons/FlatWoken/scalable/apps/xterm.svg 
! do not scroll with output
URxvt*scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

! no ISO 14755 mode (fix for C-M-v)
urxvt.iso14755:             false
urxvt.iso14755_52:          false

urxvt.font:                 xft:Monaco\ for\ Powerline:style=dz:pixelsize=18:autohint=true
urxvt.boldFont:             xft:Monaco\ for\ Powerline:style=dz:style=Bold:pixelsize=18:autohint=true
urxvt.italicFont:           xft:Monaco\ for\ Powerline:style=Italic:pixelsize=18:autohint=true
urxvt.boldItalicFont:       xft:Monaco\ for\ Powerline:style=Bold:style=Italic:pixelsize=18:autohint=true

! URxvt*font: -gohu-gohufont-medium-r-normal--14-100-100-100-c-80-iso10646-1
! URxvt*font: xft:Inconsolata -g:style=g:pixelsize=18

! --  Extensions -- !
! requires urxvt-perl package

urxvt.perl-ext-common:      default,matcher,keyboard-select,url-select,clipboard,font-size,tabbedex,resize-font
urxvt.keysym.C-x:           perl:keyboard-select:activate
urxvt.keysym.M-u:           perl:url-select:select_next
urxvt.keysym.C-b:           perl:clipboard:paste_escaped
urxvt.keysym.C-1:	        perl:font-size:increase
urxvt.keysym.M-3:           perl:font-size:decrease
urxvt.matcher.button:       1
urxvt.matcher.rend.0:       -Uline fg16
urxvt.urlLauncher:          /usr/bin/chromium
URxvt.url-select.underline: true
URxvt.url-select.launcher:  /usr/bin/xdg-open
urxvt.clipboard.autocopy:   true
urxvt.clipboard.copycmd:    xsel --input  --clipboard
urxvt.clipboard.pastecmd:   xsel --output --clipboard
urxvt.keysym.C-M-c:         perl:clipboard:copy
urxvt.keysym.C-V:           perl:clipboard:paste
urxvt.keysym.M-r:           perl:keyboard-select:search
urxvt.keysym.C-i:           perl:font-size:decrease
urxvt.keysym.C-equal:       perl:font-size:increase
urxvt.font-size.step:       4
urxvt.intensityStyles:      false
URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: -1
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: -1
URxvt.tabbed.title: no
URxvt.tabbed.new-button: no
URxvt.tabbed.autohide: yes


