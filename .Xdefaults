xterm*faceName:             Inconsolata for Powerline
xterm*scaleHeight:          1.01 
xterm*faceSize:             16
xterm*saveLines:            16384
xterm*geometry:             80x24-500+400
xterm*VT100*translations:   #override\nShift Ctrl <KeyPress> v:insert-selection(CLIPBOARD)\nShift Ctrl <KeyPress> c:copy-selection(CLIPBOARD)
xterm*boldMode:             false
xterm*allowBoldFonts:       false
xterm*utf8:                 true
xterm*termName:             xterm-256color
!xterm.VT100*utf8:           1

!                                     _   _                  _   
!                                    | | | |                | |  
!                                    | | | |_ ____  ____   _| |_ 
!                                    | | | |  __\ \/ /\ \ / / __|
!                                    | |_| | |   >  <  \ V /| |_ 
!                                     \___/|_|  /_/\_\  \_/  \__|

urxvt.letterSpace:          -1.5
urxvt.buffered:             true 
urxvt.cursorUnderline:      true
urxvt.cursorBlink:          true
urxvt.urgentOnBell:         true
urxvt.inheritPixmap:        false
urxvt.geometry:             80x24-500+400
urxvt.scrollBar:            false
urxvt.saveLines:            12000
urxvt.depth:                32
urxvt.imLocale:             en_US.UTF-8
urxvt.borderLess:           false
urxvt.pointerBlank:         true
urxvt.pointerBlankDelay:    4

urxvt.font:                 xft:Inconsolata\ for\ Powerline:style=dz:pixelsize=21:autohint=true
urxvt.boldFont:             xft:Inconsolata\ for\ Powerline:style=dz:style=Bold:pixelsize=21:autohint=true
urxvt.italicFont:           xft:Inconsolata\ for\ Powerline:style=Italic:pixelsize=21:autohint=true
urxvt.boldItalicFont:       xft:Inconsolata\ for\ Powerline:style=Bold:style=Italic:pixelsize=18:autohint=true

! URxvt*font: -gohu-gohufont-medium-r-normal--14-100-100-100-c-80-iso10646-1
! URxvt*font: xft:Inconsolata -g:style=g:pixelsize=18

! --  Extensions -- !
! requires urxvt-perl package

urxvt.perl-ext-common:      matcher,keyboard-select,url-select,clipboard
urxvt.keysym.C-Escape:      perl:keyboard-select:activate
urxvt.keysym.M-u:           perl:url-select:select_next
urxvt.keysym.C-b:           perl:clipboard:paste_escaped
urxvt.keysym.C-1:	    perl:font-size:increase
urxvt.keysym.M-3:           perl:font-size:decrease
urxvt.matcher.button:       1
urxvt.matcher.rend.0:       -Uline fg16
urxvt.urlLauncher:          chromium
urxvt.clipboard.autocopy:   true
urxvt.clipboard.copycmd:    xsel --input  --clipboard
urxvt.clipboard.pastecmd:   xsel --output --clipboard
urxvt.keysym.C-M-c:         perl:clipboard:copy
urxvt.keysym.C-V:           perl:clipboard:paste
urxvt.intensityStyles:      false

