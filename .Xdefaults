URxvt.letterSpace: -1
URxvt.buffered: true 
URxvt.cursorColor: #ffffff
URxvt.cursorUnderline: true
URxvt.cursorBlink: true 
URxvt*urgentOnBell: true
URxvt.lineSpace: 0
! URxvt*scrollBar_right: false
URxvt*scrollBar: false
URxvt*saveLines: 12000
!URxvt*font: -gohu-gohufont-medium-r-normal--14-100-100-100-c-80-iso10646-1
! URxvt*font: xft:Monospace:pixelsize=17
! URxvt.font: xft:Anonymous\ Pro-15
! URxvt*font: xft:Monaco:pixelsize=16
! URxvt*font: xft:Inconsolata:pixelsize=18:antialias=true
! URxvt*font: xft:Inconsolata -g:style=g:pixelsize=18
! Set some nice fonts.
URxvt.depth:    32
!URxvt.font: xft:Inconsolata-g:pixelsize=18
URxvt.font: xft:Consolas:pixelsize=18
!urxvt.font: \
!    xft:Inconsolata-g:style=g:pixelsize=18:autohint=true, \
!    xft:Inconsolata-g:style=g:pixelsize=18:autohint=true
!urxvt.boldFont: \
!    xft:Inconsolata-g:style=Bold:pixelsize=18:autohint=true, \
!    xft:Inconsolata-g:style=Bold:pixelsize=18:autohint=true
!urxvt.italicFont:
!    xft:Inconsolata-g:style=Italic:pixelsize=18:autohint=true, \
!    xft:Inconsolata-g:style=Italic:pixelsize=18:autohint=true
!urxvt.boldItalicFont: \
!    xft:Inconsolata-g:style=Bold:style=Italic:pixelsize=18:autohint=true, \
!    xft:Inconsolata-g:style=Bold:style=Italic:pixelsize=18:autohint=true
!! URxvt*font: xft:Inconsolata g Sans Mono:pixelsize=10
! URxvt*font: xft:Ubuntu Mono:pixelsize=18
! URxvt*font: xft:Consolas:pixelsize=18
! URxvt*font: -misc-tamsyn-medium-r-normal--20-145-100-100-c-100-iso8859-1
! URxvt*boldFont: -misc-tamsyn-bold-r-normal--20-145-100-100-c-100-iso8859-1 
URxvt.boldFont: 
URxvt*skipBuiltinGlyphs: true
URxvt*borderLess: false
!Urxvt*background: #1F1F1F
!Urxvt*foreground: #bea492
URxvt*cursorColor: #FFFFFF
URxvt*cursorColor2: #FFFFFF
URxvt*pointerBlank: true
URxvt*pointerBlankDelay: 4
URxvt*pointerColor: #FFFFFF
URxvt*pointerColor2: #FFFFFF
! URxvt.depth: 24
! --  Extensions -- !
URxvt.perl-ext-common:     matcher,keyboard-select,url-select,clipboard
URxvt.keysym.C-Escape:     perl:keyboard-select:activate
URxvt.keysym.M-u:          perl:url-select:select_next
URxvt.keysym.M-s:          perl:keyboard-select:search
URxvt.keysym.M-c:          perl:clipboard:copy_escaped  
!URxvt.keysym.C-b:          perl:clipboard:paste_escaped
URxvt.keysym.C-b:        perl:clipboard:paste
URxvt.matcher.rend.0:      -Uline fg16
URxvt.matcher.button:      1
URxvt.keysym.M-2:	perl:font-size:increase
URxvt.keysym.M-3:   perl:font-size:decrease
URxvt.url-launcher: /usr/bin/chromium
! URxvt.matcher.button: 1


URxvt.clipboard.copycmd: xsel -psb
URxvt.clipboard.pastecmd: xsel -ob
URxvt.clipboard.autocopy: true

URxvt.urlLauncher: chromium
URxvt.intensityStyles: false

