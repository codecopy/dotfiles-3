clear control
clear mod4
clear Lock

! this will deactivae escape key
keycode 9 = NoSymbol
! this will make Caps Lock to act as Esc
keycode 66 = Escape NoSymbol Escape Escape NoSymbol Escape

add mod4 = Super_L
add control = Control_L
add control = Control_R

keycode  94 = dead_circumflex degree dead_circumflex degree notsign notsign notsign
keycode  49 = less greater less greater bar brokenbar bar
